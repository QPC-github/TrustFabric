add: attributetypes
attributeTypes: ( 1.3.6.1.4.1.55988.1.2.3.615 NAME 'tf-version' EQUALITY caseIgnoreMatch SUBSTR caseIgnoreSubstringsMatch SYNTAX 1.3.6.1.4.1.1466.115.121.1.15 DESC 'Version number string')
